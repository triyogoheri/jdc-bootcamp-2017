<?xml version="1.0" encoding="UTF-8"?>
<zk>
    <listener>
        <listener-class>com.agit.jdc.shared.zul.AppInit</listener-class>
    </listener>
    <library-property>
        <name>org.zkoss.bind.DebuggerFactory.enable</name>
        <value>false</value>
    </library-property>
    <system-config>
        <id-generator-class>com.agit.jdc.util.loadtest.SequenceIdGenerator</id-generator-class>
    </system-config>
    <session-config>
        <device-type>ajax</device-type>
        <timeout-message>label:application.timeoutMessage</timeout-message>
    </session-config>
    <library-property>
        <name>org.zkoss.zkmax.tablet.ui.disabled</name>
        <value>true</value>
    </library-property>
    <language-config>
        <addon-uri>/WEB-INF/my-addon.xml</addon-uri>
    </language-config>	
</zk>
